def MeasureWater(jug1Capacity, jug2Capacity, targetCapacity):
       
        x=jug1Capacity
        y=jug2Capacity
        z=targetCapacity
        if x == z or y == z:
            return True
        if (x + y) < z:
            return False
        if (x == 0 or y == 0) and z > 0:
            return False
        if z == 0:
            return True

        # Make y to be larger bucket
        x, y = min(x, y), max(x, y)
        
        states = [0]
        print (states)
        visited = set([])
        while states:
            # State is a water level in y, while x is empty.
            state = states.pop()
            print("state:",state)
            visited.add(state)
            print("x:" , x)
            print("y:" , y)
            print("z:" , z)
            print("visited:",visited)
            if state == z or (x + state) == z:
                return True
            # Fill x full, pour water from x to y.
            s = state + x
            print("s:", s)
            
            if s <= y and s not in visited:
                states.append(s)
                print("states",states)
            # Pour water from y to x, empty x.
            s = state - x
            print("s:", s)
            if s > 0 and s not in visited:
                states.append(s)
                print("states",states)
            # Pour water from y to x, fill y full, empty x.
            s = y - (x - state)
            print("s:", s)
            if s > 0 and s <= y and s not in visited:
                states.append(s)
                print("states",states)
            # Fill x, pour from x to y, empty y.
            s = x - (y - state)
            print("s:", s)
            if s > 0 and s <= y and s not in visited:
                states.append(s)
                print("states:",states)
                print("s:", s)
        return False
MeasureWater(4,3,2)
